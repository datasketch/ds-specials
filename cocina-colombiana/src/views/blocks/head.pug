<!DOCTYPE html>
html(lang="es")
	head
		meta(charset="UTF-8")
		meta(name="viewport", content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0")
		meta(http-equiv="X-UA-Compatible", content="IE=7")
		block meta
		block title
		link(rel="shortcut icon" href="favicon.ico" type="image/x-icon")
		link(rel="icon" href="favicon.ico" type="image/x-icon")
		link(rel="stylesheet", href="vendors/hamburgers.min.css")
		link(rel="stylesheet", href="assets/css/shared.css")
		block css
		script.
			window.fbAsyncInit = function() {
				FB.init({
					appId      : '283256382197189',
					xfbml      : true,
					version    : 'v2.11'
				});
				FB.AppEvents.logPageView();
			};

			(function(d, s, id){
				var js, fjs = d.getElementsByTagName(s)[0];
				if (d.getElementById(id)) {return;}
				js = d.createElement(s); js.id = id;
				js.src = "https://connect.facebook.net/es_CO/sdk.js";
				fjs.parentNode.insertBefore(js, fjs);
			}(document, 'script', 'facebook-jssdk'));
	body
		block header
		#wrapper.flex.flex--column
			main
				block main
			block footer
		script(src="vendors/jquery.min.js")
		script(src="assets/js/shared.js")
		block js